---
permalink: rules.json
---

[ {% for rule in rules %}
    {
        "id":"{{rule.name | slugify}}",
        "name":"{{ rule.name }}",
        "description":{{ rule.description | mdDescription }},
        "link":"/rules/{{rule.name | slug }}/",
        "source":"{{rule.source}}",
        "page":{{rule.page}},
        "version":{{rule.version}},
        "errata":{{rule.errata}}
    }{% unless forloop.last%},{% endunless %}
{% endfor %} ]
